{
  "version": 2,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/client/$1"
    }
  ],
  "env": {
    "SEOUL_METRO_API_KEY": "@seoul-metro-api-key"
  },
  "functions": {
    "server/index.js": {
      "maxDuration": 30
    }
  }
}
